{
  "name": "react-playground",
  "version": "0.0.1",
  "description": "My very personal opinionated Isomorphic React Golden Setup",
  "author": "Marco Bettiolo <marco@bettiolo.it>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bettiolo/react-playground"
  },
  "scripts": {
    "build": "npm run lint && node --harmony ./node_modules/gulp/bin/gulp.js build",
    "lint": "eslint --ext .js,.jsx ./",
    "start": "DEBUG=app,app:* node --harmony --use-strict ./server.js"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "devDependencies": {
    "babel-eslint": "^3.0.1",
    "babelify": "^6.0.2",
    "browserify": "^10.1.0",
    "browserify-shim": "^3.8.6",
    "del": "^1.1.1",
    "eslint": "^0.20.0",
    "gulp": "^3.8.11",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "analyze": true,
  "license": "MIT",
  "dependencies": {
    "babel": "^5.2.16",
    "debug": "^2.1.3",
    "ejs": "^2.3.1",
    "express": "^4.12.3",
    "react": "^0.13.2"
  }
}
